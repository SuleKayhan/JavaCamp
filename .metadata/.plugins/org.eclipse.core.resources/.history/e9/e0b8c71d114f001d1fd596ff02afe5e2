
public class miniProjeAsalSayi {

	public static void main(String[] args) {
		int number = 25;
		int remainder ; // remainder = kalan demek
		int i;
		boolean isPrime = true; // Prime = asal demek. Asal mı = evet.
		
		for (i=2; i < number; i++)
		{
			if(int deger = number % i == 0)
				System.out.println("asal değil");
			System.out.println(number);
		}

	}

}
