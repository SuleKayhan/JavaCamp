
public class miniProjeAsalSayi {

	public static void main(String[] args) {
		int number = 23;
		int remainder ; // remainder = kalan demek
		int i;
		boolean isPrime = true; // Prime = asal demek. Asal mÄ± = evet.
		
		for (i=2; i < number; i++)
		{
			if(number % i == 0)
				isPrime = false;
			
		}

	}

}
